{
  "name": "office-js-helpers",
  "description": "An open-source collection of helpers for developing Office Add-ins",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OfficeDev/office-js-helpers.git"
  },
  "scripts": {
    "postinstall": "typings install",
    "build": "rimraf dist && tsc -p tsconfig.json && npm run browserify && npm run uglify",
    "browserify": "node browserify.js",
    "uglify": "uglifyjs dist/umd.js --compress --source-map --output dist/umd.min.js",
    "lint": "tslint 'src/**/*.ts'"
  },
  "version": "1.0.0",
  "main": "src/index.ts",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "dependencies": {
    "core-js": "^2.4.1"
  },
  "devDependencies": {
    "typings": "^1.3.3",
    "typescript": "^1.8.10",
    "rimraf": "^2.5.4",
    "tslint": "^3.15.1",
    "browserify": "^13.0.0",
    "browserify-umdify": "^1.0.3",
    "glob": "^7.0.0",
    "mkdirp": "^0.5.1",
    "uglify-js": "^2.6.2",
    "util": "^0.10.3"
  },
  "keywords": [
    "OfficeJS",
    "Helpers"
  ],
  "author": "Microsoft",
  "bugs": {
    "url": "https://github.com/OfficeDev/office-js-helpers/issues"
  },
  "homepage": "https://github.com/OfficeDev/office-js-helpers#readme"
}